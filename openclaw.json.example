    { "match": { "channel": "telegram", "peer": { "kind": "group", "id": "TG_GROUP_MERRY_ID" } }, "agentId": "merry-bot" },
    { "match": { "channel": "telegram", "peer": { "kind": "group", "id": "TG_GROUP_ASSISTANT_DASHBOARD_ID" } }, "agentId": "assistant" },
    { "match": { "channel": "telegram", "peer": { "kind": "direct", "id": "TG_OWNER_ID" } }, "agentId": "main" }
