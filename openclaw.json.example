      "groupAllowFrom": [
        "TG_GROUP_CODER_ID",
        "TG_GROUP_HELLO_ID",
        "TG_GROUP_ANXIETY_CHAT_ID",
        "TG_GROUP_MERRY_ID",
        "TG_GROUP_ASSISTANT_DASHBOARD_ID"
      ],
      "groups": {
        "TG_GROUP_CODER_ID": { "requireMention": false },
        "TG_GROUP_HELLO_ID": { "requireMention": false },
        "TG_GROUP_ANXIETY_CHAT_ID": { "requireMention": false },
        "TG_GROUP_MERRY_ID": { "requireMention": false },
        "TG_GROUP_ASSISTANT_DASHBOARD_ID": { "requireMention": false }
      }
    }
  },
  "bindings": [
    { "match": { "channel": "telegram", "peer": { "kind": "group", "id": "TG_GROUP_CODER_ID" } }, "agentId": "g-coder" },
    { "match": { "channel": "telegram", "peer": { "kind": "group", "id": "TG_GROUP_HELLO_ID" } }, "agentId": "g-hello" },
